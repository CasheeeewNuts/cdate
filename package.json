{
  "name": "cdate",
  "description": "a compact calendar date",
  "version": "1.0.0",
  "author": "@kawanet",
  "browser": "./dist/cdate.min.js",
  "bugs": {
    "url": "https://github.com/kawanet/cdate/issues"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.9",
    "@types/strftime": "^0.9.4",
    "browserify": "^17.0.0",
    "browserify-sed": "^0.8.0",
    "dayjs": "^1.11.6",
    "mocha": "^10.1.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.39",
    "strftime": "^0.10.1",
    "terser": "^5.16.1",
    "typescript": "^4.9.3"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./dist/cdate.js"
    },
    "./locale/*.js": {
      "import": "./locale/*.js",
      "require": "./dist/locale/*.js"
    }
  },
  "files": [
    "browser/import.cjs",
    "dist/cdate.js",
    "dist/cdate.min.js",
    "dist/locale/*.js",
    "dist/package.json",
    "index.js",
    "locale/*.js",
    "src/*.js",
    "types/cdate.d.ts"
  ],
  "homepage": "https://github.com/kawanet/cdate#readme",
  "keywords": [
    "calendar",
    "date",
    "moment",
    "strftime",
    "time"
  ],
  "license": "MIT",
  "main": "./dist/cdate.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kawanet/cdate.git"
  },
  "scripts": {
    "build": "make -C browser clean all",
    "build-locale": "make -C locale clean all",
    "fixpack": "fixpack",
    "prepack": "npm run build && npm test && npm run test-minified && npm run test-timezone",
    "test": "mocha test",
    "test-minified": "make -C browser mocha",
    "test-timezone": "for tz in Asia/Tokyo Asia/Kathmandu Europe/London America/St_Johns America/Los_Angeles; do echo TZ=$tz mocha test; TZ=$tz mocha test || break; done"
  },
  "type": "module",
  "types": "types/cdate.d.ts"
}
