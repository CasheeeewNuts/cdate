{
  "include": [
    "../test/"
  ],
  "compilerOptions": {
    "esModuleInterop": true,
    "module": "CommonJS",
    "moduleResolution": "Node",
    "noImplicitAny": true,
    "noImplicitReturns": false,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "../build/test/",
    "removeComments": true,
    "skipLibCheck": false,
    "strictNullChecks": false,
    "target": "ES6"
  }
}
